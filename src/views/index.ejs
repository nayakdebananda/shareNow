<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/style.css" />
    <title>Share Now</title>
  </head>
  <body>
    <%- include('nav.ejs') %> <% files.forEach(file =>{ %>
    <section>
      <a href="/download/<%= file %>"><%= file %> </a>
    </section>

    <% }) %> <% if (files.length<1) { %>
    <section>
      <a href="/upload">Send Some Files</a>
    </section>
    <% } %>
  </body>
</html>
